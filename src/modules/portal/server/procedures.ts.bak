      console.log(
        `ðŸ‘¤ Found matching resident: ${matchingResident.firstName} ${matchingResident.lastName} (${matchingResident.id})`
      );      try {
        if (existingUser) {
          // Update existing user record - FIXED: Only update by userId, ensure orgId is set
          console.log(`ðŸ”„ Updating existing user: ${userId}`);
          
          const updateResult = await db
            .update(users)
            .set({
              orgId, // Ensure orgId is set or updated
              residentId: matchingResident.id,
              name: `${matchingResident.firstName} ${matchingResident.lastName}`,
              email: matchingResident.email,
              phone: matchingResident.phone,
              updatedAt: new Date(),
            })
            .where(eq(users.userId, userId)) // FIXED: Only filter by userId
            .returning();
          
          console.log(`âœ… Update result:`, updateResult);
          console.log(`âœ… Rows updated: ${updateResult.length}`);
          
          if (updateResult.length === 0) {
            console.warn(`âš ï¸ Update did not affect any rows for userId: ${userId}`);
            console.log(`ðŸ” Verifying user exists...`);
            
            // Double-check if the user exists
            const checkResult = await db
              .select()
              .from(users)
              .where(eq(users.userId, userId))
              .limit(1);
              
            if (checkResult.length === 0) {
              console.error(`âŒ User not found after update attempt. Creating new record.`);
              await db.insert(users).values({
                userId: userId,
                orgId: orgId,
                name: `${matchingResident.firstName} ${matchingResident.lastName}`,
                email: matchingResident.email,
                phone: matchingResident.phone,
                residentId: matchingResident.id,
              });
            } else {
              console.error(`âŒ User exists but update failed. Manual fix required.`);
            }
          }
        } else {
          // Create new user record with resilient handling
          console.log(`ðŸ†• Creating new user: ${userId}`);
          try {
            await db.insert(users).values({
              userId: userId,
              orgId: orgId,
              name: `${matchingResident.firstName} ${matchingResident.lastName}`,
              email: matchingResident.email,
              phone: matchingResident.phone,
              residentId: matchingResident.id,
            });
            console.log(`âœ… New user created`);
          } catch (insertError) {
            // Handle duplicate key errors 
            if (isDuplicateKeyError(insertError)) {
              console.log(`âš ï¸ Duplicate key detected, attempting update instead`);
              // Try to update instead
              await db
                .update(users)
                .set({
                  orgId, // Ensure orgId is set or updated 
                  residentId: matchingResident.id,
                  name: `${matchingResident.firstName} ${matchingResident.lastName}`,
